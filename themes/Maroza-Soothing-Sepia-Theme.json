{
    "name": "Maroza Soothing Sepia (Refined)",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "enumMember": {
            "foreground": "#FABD2F"
        },
        "variable.constant": {
            "foreground": "#D3869B"
        },
        "variable.defaultLibrary": {
            "foreground": "#FABD2F"
        }
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#928374"
            }
        },
        {
            "name": "Keywords & Storage Types",
            "scope": "keyword, storage.type, storage.modifier, keyword.control.flow, keyword.control.conditional, keyword.control.loop, keyword.operator.new",
            "settings": {
                "foreground": "#FB4934"
            }
        },
        {
            "name": "Types, Classes, Modules, and Language Objects",
            "scope": "entity.name.type, entity.name.class, support.class, support.type, support.module, support.type.object, variable.other.object",
            "settings": {
                "foreground": "#FABD2F"
            }
        },
        {
            "name": "Functions & Methods",
            "scope": "entity.name.function, support.function, meta.function-call.generic",
            "settings": {
                "foreground": "#8EC07C"
            }
        },
        {
            "name": "Object Properties, Keys, and Member Variables",
            "scope": "variable.other.property, meta.object-literal.key, support.variable.property, entity.name.variable.field",
            "settings": {
                "foreground": "#83A598"
            }
        },
        {
            "name": "Variables (default)",
            "scope": "variable.other, variable.other.readwrite",
            "settings": {
                "foreground": "#EBDBB2"
            }
        },
        {
            "name": "Parameters",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#D5C4A1",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Language Variables & Special Keywords",
            "scope": "variable.language, keyword.other.special-method",
            "settings": {
                "foreground": "#FE8019",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings & Template Literals",
            "scope": "string, punctuation.definition.string",
            "settings": {
                "foreground": "#B8BB26"
            }
        },
        {
            "name": "Numbers & Language Constants",
            "scope": "constant.numeric, constant.language",
            "settings": {
                "foreground": "#D3869B"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#FE8019"
            }
        },
        {
            "name": "Punctuation & Delimiters",
            "scope": "punctuation, meta.brace",
            "settings": {
                "foreground": "#A89984"
            }
        },
        {
            "name": "HTML/XML Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#FB4934"
            }
        },
        {
            "name": "HTML/XML Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#FABD2F"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": "meta.selector, entity.other.attribute-name.class, entity.name.tag.css",
            "settings": {
                "foreground": "#83A598"
            }
        },
        {
            "name": "CSS Property Names",
            "scope": "support.type.property-name.css",
            "settings": {
                "foreground": "#EBDBB2"
            }
        },
        {
            "name": "CSS Property Values",
            "scope": "support.constant.property-value.css",
            "settings": {
                "foreground": "#FE8019"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": "markup.heading",
            "settings": {
                "foreground": "#8EC07C",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Link",
            "scope": "markup.underline.link.markdown, string.other.link.title.markdown",
            "settings": {
                "foreground": "#83A598"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": "markup.bold",
            "settings": {
                "foreground": "#FE8019",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": "markup.italic",
            "settings": {
                "foreground": "#FB4934",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#928374",
                "fontStyle": "italic"
            }
        },
        {
            "name": "PHP Tags (<?php ?>)",
            "scope": "punctuation.section.embedded.begin.php, punctuation.section.embedded.end.php",
            "settings": {
                "foreground": "#FB4934",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Python `self`",
            "scope": "variable.parameter.function.language.special.self.python",
            "settings": {
                "foreground": "#FE8019",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Python Decorators",
            "scope": "entity.name.function.decorator",
            "settings": {
                "foreground": "#FABD2F"
            }
        },
        {
            "name": "JS/TS Italic Contexts",
            "scope": "entity.other.attribute-name.js, variable.language.super",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "diff inserted",
            "scope": "markup.inserted.diff",
            "settings": {
                "foreground": "#B8BB26"
            }
        },
        {
            "name": "diff deleted",
            "scope": "markup.deleted.diff",
            "settings": {
                "foreground": "#FB4934"
            }
        },
        {
            "name": "diff changed",
            "scope": "markup.changed.diff",
            "settings": {
                "foreground": "#FABD2F"
            }
        }
    ],
